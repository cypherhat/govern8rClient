<RegisterWalletScreen>:
    FloatLayout:
        Label:
            text: "Notary - Register "
            pos_hint: {'x': .1, 'y': .8}
            size_hint: .425, .1
            font_size: '30sp'
        Label:
            text: "Email : "
            pos_hint: {'x': .1, 'y': .7}
            size_hint: .23, .1
            halign: 'left'
            font_size: '20sp'
        TextInput:
            id: email
            pos_hint: {'x': .3, 'y': .7}
            size_hint: .6, .1
            font_size: '40sp'
        Button:
            text: 'Register'
            pos_hint: {'x': .3, 'y': .5}
            size_hint: .3, .1
            font_size: '20sp'
            on_press: root.register_wallet_callback()