<SelectNotaryFileScreen>:
    FloatLayout:
        FileChooserIconView:
            id: filechooser
            show_hidden: True
            on_selection: root.my_callback()
            size: .9, .6
        Label:
            text: "Selected File:"
            pos_hint: {'x': .0, 'y': .0}
            size_hint: .7, .1
            halign: 'left'
            font_size: '20sp'